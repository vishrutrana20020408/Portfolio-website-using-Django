{
  "builds": [
    {
      "src": "Portfolio/asgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb", "runtime": "python3.12" }
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "Portfolio/asgi.py" }
  ]
}
